{
  "name": "@examples/trpc-next-prisma-starter",
  "version": "10.9.0",
  "private": true,
  "scripts": {
    "migrate-sqlite": "prisma migrate dev --name init --schema=./prisma/_sqlite/schema.prisma",
    "migrate-postgres": "prisma migrate dev",
    "generate-sqlite": "prisma generate --schema=./prisma/_sqlite/schema.prisma",
    "generate-postgres": "prisma generate",
    "generate-mongodb": "prisma generate --schema=./prisma/schema.prisma",
    "prisma-studio-sqlite": "prisma studio --schema=./prisma/_sqlite/schema.prisma",
    "prisma-studio-postgres": "prisma studio",
    "db-seed": "prisma db seed",
    "db-reset": "prisma migrate reset",
    "dx:next": "run-s migrate-sqlite generate-sqlite db-seed && next dev",
    "dx:next2": "run-s generate-mongodb && next dev",
    "dx:prisma-studio": "pnpm prisma-studio-sqlite",
    "dx": "run-p dx:* --print-label",
    "dev": "pnpm dx:next2",
    "prep-sqlite": "run-s generate-sqlite migrate-sqlite",
    "build-sqlite": "next build",
    "build-postgres": "run-s generate-postgres migrate && next build",
    "build-mongodb": "run-s generate-mongodb && next build",
    "build": "pnpm build-mongodb",
    "start": "next start",
    "lint": "eslint --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "pnpm lint --fix",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test 'next dev' http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true,
    "endOfLine": "auto"
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mantine/core": "^5.10.1",
    "@mantine/form": "^5.10.1",
    "@mantine/hooks": "^5.10.1",
    "@mantine/next": "^5.10.1",
    "@prisma/client": "^4.9.0",
    "@tabler/icons": "^2.1.2",
    "@tanstack/react-query": "^4.3.8",
    "@trpc/client": "^10.9.0",
    "@trpc/next": "^10.9.0",
    "@trpc/react-query": "^10.9.0",
    "@trpc/server": "^10.9.0",
    "@types/bcryptjs": "^2.4.2",
    "aws-sdk": "^2.1332.0",
    "bcryptjs": "^2.4.3",
    "bson": "^4.7.2",
    "classnames": "^2.3.2",
    "clsx": "^1.1.1",
    "emoji-picker-react": "^4.4.7",
    "next": "^13.0.0",
    "nookies": "^2.5.2",
    "pnpm": "^7.25.1",
    "progressbar.js": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-infinite-scroller": "^1.2.6",
    "superjson": "^1.7.4",
    "timeago.js": "^4.0.2",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.26.1",
    "@types/node": "^18.7.20",
    "@types/progressbar.js": "^1.1.2",
    "@types/react": "^18.0.9",
    "@types/react-infinite-scroller": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.7.1",
    "start-server-and-test": "^1.12.0",
    "tsx": "^3.9.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vitest": "^0.28.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
